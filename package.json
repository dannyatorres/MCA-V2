{
  "name": "mca-command-center",
  "version": "1.0.1",
  "description": "MCA Command Center - Unified SMS automation, FCS processing, and lender qualification system",
  "main": "backend/server-new.js",
  "scripts": {
    "start": "node backend/server-new.js",
    "build": "echo 'No build step needed'",
